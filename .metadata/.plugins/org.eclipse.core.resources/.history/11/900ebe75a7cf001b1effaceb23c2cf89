package modelado;

public class Empleado extends Persona{
	
	private float sueldo;
	private String obraSocial;
	private String telefono;
	private String nroAsociado;
	
	
	public Empleado(float sueldo, String obraSocial, String telefono, String nroAsociado) {
		super();
		this.sueldo = sueldo;
		this.obraSocial = obraSocial;
		this.telefono = telefono;
		this.nroAsociado = nroAsociado;
	}


	public Empleado(String nombre, String apellido, StringBuilder cuil, String nacionalidad, String direccion,
			float sueldo, String obraSocial, String telefono, String nroAsociado) {
		super(nombre, apellido, cuil, nacionalidad, direccion);
		this.sueldo = sueldo;
		this.obraSocial = obraSocial;
		this.telefono = telefono;
		this.nroAsociado = nroAsociado;
	}
	
	
	
	
	
}
