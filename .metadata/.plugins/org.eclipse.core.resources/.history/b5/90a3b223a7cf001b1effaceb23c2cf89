package modelado;


public class Profesor extends Persona{
	
	
	private String nroAsociado;
	private String obraSocial;
	private float sueldo;
	private int cantidadDeCursosCargo;
	private String telefono;
	private String materiaQueEnsenia;
	
	
	
	public Profesor(String nroAsociado, float sueldo, int cantidadDeCursosCargo, String telefono,
			String materiaQueEnsenia) {
		super();
		this.nroAsociado = nroAsociado;
		this.obraSocial = obraSocial;
		this.sueldo = sueldo;
		this.cantidadDeCursosCargo = cantidadDeCursosCargo;
		this.telefono = telefono;
		this.materiaQueEnsenia = materiaQueEnsenia;
	}
	

	public Profesor(String nombre, String apellido, StringBuilder cuil, String nacionalidad, String direccion,
			String nroAsociado, float sueldo, int cantidadDeCursosCargo, String telefono, String materiaQueEnsenia) {
		super(nombre, apellido, cuil, nacionalidad, direccion);
		this.nroAsociado = nroAsociado;
		this.obraSocial = obraSocial;
		this.sueldo = sueldo;
		this.cantidadDeCursosCargo = cantidadDeCursosCargo;
		this.telefono = telefono;
		this.materiaQueEnsenia = materiaQueEnsenia;
	}

	public void setObraSocial(String obraSocial) {
		this.obraSocial = obraSocial;
	}
	
	public String getObraSocial() {
		return obraSocial;
	}

	public float getSueldo() {
		return sueldo;
	}
	public void setSueldo(float sueldo) {
		this.sueldo = sueldo;
	}
	public int getCantidadDeCursosCargo() {
		return cantidadDeCursosCargo;
	}
	public void setCantidadDeCursosCargo(int cantidadDeCursosCargo) {
		this.cantidadDeCursosCargo = cantidadDeCursosCargo;
	}
	public String getTelefono() {
		return telefono;
	}
	public void setTelefono(String telefono) {
		this.telefono = telefono;
	}
	public String getNroAsociado() {
		return nroAsociado;
	}
	public String getMateriaQueEnsenia() {
		return materiaQueEnsenia;
	}


	@Override
	public String toString() {
		return "Profesores [nroAsociado=" + nroAsociado + ", sueldo=" + sueldo + ", cantidadDeCursosCargo="
				+ cantidadDeCursosCargo + ", telefono=" + telefono + ", materiaQueEnsenia=" + materiaQueEnsenia + "]"+ super.toString();
	}
	
	
	
}

