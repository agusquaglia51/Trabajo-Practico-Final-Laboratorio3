package modelado;

public class EntradaMapa<K,V> {
	
	    private K clave;
	    private V valor;

	    public EntradaMapa() {
	        // TODO Auto-generated constructor stub
	    }
	    public void setClave(K clave) {
	        this.clave = clave;
	    }
	    public void setValor(V valor) {
	        this.valor = valor;
	    }
	    public K getClave() {
	        return clave;
	    }
	    public V getValor() {
	        return valor;
	    }

}

